<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="volumeSettings" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.options.options">
   <Label fx:id="label_general" layoutX="69.0" layoutY="127.0" text="general" />
   <Label fx:id="label_effects" layoutX="69.0" layoutY="213.0" text="effects" />
   <Label fx:id="label_music" layoutX="69.0" layoutY="300.0" text="music" />
   <CheckBox layoutX="762.0" layoutY="127.0" mnemonicParsing="false" text="Mute" />
   <CheckBox layoutX="762.0" layoutY="213.0" mnemonicParsing="false" text="Mute" />
   <CheckBox layoutX="762.0" layoutY="300.0" mnemonicParsing="false" text="Mute" />
   <Slider fx:id="slider_general" layoutX="270.0" layoutY="111.0" onMouseReleased="#onGeneralChanged" prefHeight="25.0" prefWidth="400.0" showTickLabels="true" showTickMarks="true" />
   <Slider fx:id="slider_effects" layoutX="270.0" layoutY="197.0" onMouseReleased="#onEffectsChanged" prefHeight="25.0" prefWidth="400.0" showTickLabels="true" showTickMarks="true" />
   <Slider fx:id="slider_music" layoutX="270.0" layoutY="284.0" onMouseReleased="#onMusicChanged" prefHeight="25.0" prefWidth="400.0" showTickLabels="true" showTickMarks="true" />
</Pane>
